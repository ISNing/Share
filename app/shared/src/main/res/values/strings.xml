        <item quantity="one">Succeeded sending file to <xliff:g id="target_name" example="ISNing's Tablet">%1$s</xliff:g></item>
        <item quantity="other">Succeeded sending files to <xliff:g id="target_name" example="ISNing's Tablet">%1$s</xliff:g></item>
