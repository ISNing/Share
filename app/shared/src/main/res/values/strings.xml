    <string name="notification_title_receive_request">Share Request</string>
    <plurals name="notification_text_receive_request">
        <item quantity="one">\"<xliff:g id="sender_name" example="ISNing's Phone">%1$s</xliff:g>\" want\'s to share <xliff:g id="file_count" example="1">%2$d</xliff:g> file with you</item>
        <item quantity="other">\"<xliff:g id="sender_name" example="ISNing's Phone">%1$s</xliff:g>\" want\'s to share <xliff:g id="file_count" example="3">%2$d</xliff:g> files with you</item>
    </plurals>
    <plurals name="notification_text_expanded_receive_request">
        <item quantity="other">\"<xliff:g id="sender_name" example="ISNing's Phone">%1$s</xliff:g>\" want\'s to share \"<xliff:g id="file_names" example="base.apk\nexTHmUI-OFFICIAL.zip">%2$s</xliff:g>\" with you</item>
    </plurals>

