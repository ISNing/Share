    <plurals name="notification_text_sending_failed">
        <item quantity="other">Reason: <xliff:g id="reason">%1$s</xliff:g></item>
    </plurals>
    <plurals name="notification_text_expanded_sending_failed">
        <item quantity="one">Reason: <xliff:g id="reason">%1$s</xliff:g>\nFile: \"<xliff:g id="file_name" example="base.apk">%2$s</xliff:g>\"</item>
