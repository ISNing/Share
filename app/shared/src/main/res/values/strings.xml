    <!-- Sending failed notification -->
    <plurals name="notification_title_sending_failed">
        <item quantity="one">Failed sending file to <xliff:g id="target_name" example="ISNing's Tablet">%1$s</xliff:g></item>
        <item quantity="other">Failed sending files to <xliff:g id="target_name" example="ISNing's Tablet">%1$s</xliff:g></item>
